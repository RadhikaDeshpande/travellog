<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<title>Share your travel experience</title>

    <link href="bootstrap3/css/bootstrap.css" rel="stylesheet" />
    <link href="assets/css/get-shit-done.css" rel="stylesheet" />
    <link href="assets/css/jquery-ui.css" rel="stylesheet" />
    <link href="assets/css/demo.css" rel="stylesheet" />

    <!--     Font Awesome     -->
    <link href="bootstrap3/css/font-awesome.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Grand+Hotel' rel='stylesheet' type='text/css'>

</head>
<body>
<form id="uploadForm">
<div id="navbar-full">
  <div id="navbar">
    <div class="navigation-example">
      <nav class="navbar navbar-default navbar-transparent" role="navigation-demo">
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header">
            <a class="navbar-brand" href="http://localhost:3000/">TravelLog </a>
            <a class="navbar-brand" style='margin-left:5px;'>
            <!-- <% if (message.length > 0) { %><div class="row divtext"><%= message %></div><% } %>  -->
            <% if (user_id.length  > 0 && user_name.length > 0) { %><div class="row divtext"> Welcome <%= user_id %>!, <%= user_name %> </div><% } %> 
            <!-- <% if (user_name.length > 0) { %><div class="row divtext">Have a great time<%= user_name %>!</div><% } %> </a> -->
          </div>
          <div class="logoutButton">
            <a class="navbar-brand" href="http://localhost:3000/logout">Logout</a>
          </div>
        </div>

        <div class="travelBlogEntry">
          <div class="travelTitle"><h4>Tell us about your travel experience</h4></div>
          <textarea name="travel_text" class="form-control custom-control" rows="5" style="resize:none" placeholder="Enter your travel Experience"></textarea> 
          <div class="travelRow">
            <div class="travelRowLeft">
              Enter City <input class="ff_elem" type="text" name="ff_nm_from[]" value="" id="f_elem_city"/>
            </div>

            <div class="travelRowRight">
              <div class="travelImageText">Upload your Fav Travel Image</div>
              <div class="travelImageButton"><input type="file" name="travel_image" class="btn btn-block btn-default"></input></div>
            </div>

          </div>
        </div>

        <div class="foodBlogEntry">
          <div class="foodTitle"><h4>Tell us about your food experience</h4></div>
          <input name="food_joint_name" class="form-control custom-control" placeholder="Name of the establishment"></input> 
          <textarea name="food_text" class="form-control custom-control" rows="5" style="resize:none" placeholder="Enter your food Experience"></textarea> 

          <div class="foodRow">          
            <div class="foodRowRight">
              <div class="foodImageText">Upload your Fav Food Image</div>
              <div class="foodImageButton"><input type="file" name="food_image" class="btn btn-block btn-default"></input></div>
            </div>

          </div>
        </div>

        <input type="hidden" name="travel_image_count" value="1">
        <input type="hidden" name="food_image_count" value="1">
        <input type="hidden" name="action_type" value="1">
        <input type="hidden" name="location" value="Chicago">
        <input type="hidden" id="city" name="city" value="">
        <input type="hidden" id="state" name="state" value="">
        <input type="hidden" id="country" name="country" value="">
        <input type="hidden" id="cityDetails" name="cityDetails" value="">
        <% if (user_id.length  > 0 ) { %>
        <input type="hidden" name="email_id" value="<%= user_id %>">
        <% } %>
        <input id="submitButton" type="submit" class="btn btn-default btn-block"></input>
        
      </nav>    
    </div>
  </div><!--  end navbar -->
</div> <!-- end menu-dropdown -->
</form>


</body>

  <script src="jquery/jquery-1.10.2.js" type="text/javascript"></script>
  <script src="jquery/jquery-ui.min.js"></script>
	<script src="assets/js/jquery-ui-1.10.4.custom.min.js" type="text/javascript"></script>

	<script src="bootstrap3/js/bootstrap.js" type="text/javascript"></script>
	<script src="assets/js/gsdk-checkbox.js"></script>
	<script src="assets/js/gsdk-radio.js"></script>
	<script src="assets/js/gsdk-bootstrapswitch.js"></script>
	<script src="assets/js/get-shit-done.js"></script>

  <script src="assets/js/custom.js"></script>

<script type="text/javascript">

    $('.btn-tooltip').tooltip();
    $('.label-tooltip').tooltip();
    $('.pick-class-label').click(function(){
        var new_class = $(this).attr('new-class');
        var old_class = $('#display-buttons').attr('data-class');
        var display_div = $('#display-buttons');
        if(display_div.length) {
        var display_buttons = display_div.find('.btn');
        display_buttons.removeClass(old_class);
        display_buttons.addClass(new_class);
        display_div.attr('data-class', new_class);
        }
    });
    $( "#slider-range" ).slider({
		range: true,
		min: 0,
		max: 500,
		values: [ 75, 300 ],
	});
	$( "#slider-default" ).slider({
			value: 70,
			orientation: "horizontal",
			range: "min",
			animate: true
	});
	$('.carousel').carousel({
      interval: 4000
    });


</script>
</html>
